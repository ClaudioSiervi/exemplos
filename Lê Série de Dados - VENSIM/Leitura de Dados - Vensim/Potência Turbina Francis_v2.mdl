Erro=
	ABS(Potencia-Potencia Simulada)
	~	
	~		|

gravidade=
	9.8
	~	m/s²
	~		|

Potencia:=
	GET XLS DATA(NomeArquivo, 'Dados' , 'a' , 'b2' )
	~	MW
	~	Potência gerada pela turbina 
	|

Potencia Simulada=
	gravidade*Queda Líquida*Rendimento*Vazão/1000
	~	MW
	~		|

Queda Líquida:=
	GET XLS DATA(NomeArquivo, 'Dados' , 'a' , 'e2' )
	~	m
	~		|

Rendimento:=
	GET XLS DATA(NomeArquivo, 'Dados' , 'a' , 'c2' )
	~	pu
	~	Rendimento da turbina
	|

Vazão:=
	GET XLS DATA(NomeArquivo, 'Dados' , 'a' , 'd2' )
	~	m³/s
	~	Vazão turbinada
	|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 100
	~	Minute
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Minute
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP 
	~	Minute [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	Minute [0,?]
	~	The time step for the simulation.
	|

********************************************************
	.Dados de Potencia
********************************************************~
	|

NomeArquivo:IS:
	'Pontos_Polinomio_Rendimento.xlsx'
	~	
	~		|

